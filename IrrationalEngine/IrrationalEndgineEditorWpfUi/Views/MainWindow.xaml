<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IrrationalEndgineEditorWpfUi"
        xmlns:CustomComponents="clr-namespace:IrrationalEndgineEditorWpfUi.CustomComponents" x:Class="IrrationalEndgineEditorWpfUi.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
        <StackPanel>
                    <Menu DockPanel.Dock="Top">
                        <MenuItem Header="_File">
                            <MenuItem Header="_New" Click="NewWindow"/>
                            <MenuItem Header="_Open" />
                            <MenuItem Header="_Save" />
                            <Separator />
                            <MenuItem Header="_Exit" />
                        </MenuItem>
                    </Menu>
            <CustomComponents:ObjectBrowser Width="Auto" Height="Auto"/>
        </StackPanel>
</Window>
